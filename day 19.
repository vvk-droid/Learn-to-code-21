#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>


void swap(int m,int n){
 int temp;
    temp = m;
    m = n;
    n = temp;
    printf("a=%d b=%d",m,n);
}

int main() 
{
    int a,b;
    
    scanf("%d %d",&a,&b);
    
    printf("a=%d b=%d\n",a,b);
    
    swap(a,b);
    
    return 0;}
