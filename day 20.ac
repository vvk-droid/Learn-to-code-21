#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int factorial(n) {
   if(n == 0)
        return 1;
    else 
        return (n*factorial(n-1)); 
}

int main() 
{
    int num;
    scanf("%d",&num);
   
    if(num>=0)
        printf("%d",factorial(num));
    else
        printf("ERROR");
    
    return 0;
}
