#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
  char arr[100];
    
    fgets(arr,100,stdin);
    int i,space=0,vowel= 0, cons=0;
    for(i=0; arr[i] != '\0'; i++)
    {  
    if(arr[i] == ' ')
     space++;
    else if( arr[i]=='a' || arr[i]=='e' ||arr[i]=='i' ||arr[i]=='o' ||arr[i]=='u')
     vowel ++;
    else 
     cons++;
    }
    
    printf( "%d\n%d\n%d",vowel,cons,space);
    
  
    return 0;
    
    
}
