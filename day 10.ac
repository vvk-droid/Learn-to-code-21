#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int a,b,c,d;
    
    scanf("%d",&a);
    
    for(b=a;b>=1;b--)
    {
        for(c=a;c>b;c--)
        {
            printf(" ");
        }
        for(d=1;d<=b;d++)
        {
            printf("%d",b);
        }
        
        printf("\n");
    }

        
    return 0;
    
    
    
    
    
    
}
