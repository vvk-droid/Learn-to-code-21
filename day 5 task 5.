#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int v;
     scanf("%d",&v);
    
    
    
    if((v%400==0)||((v%4==0)&&(v%100!=0)))
        
        
        
        printf("Yes");
    
    
    else
        
        
        printf("No");

       
    return 0;
    
    
}
